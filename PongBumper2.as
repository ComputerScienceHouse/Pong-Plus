package code{	import flash.display.*;	import flash.events.*;	import flash.geom.Point;	public class PongBumper2 extends MovieClip	{		var startX:Number = 300;		var startY:Number = 100;		var center:Point = new Point(0,0);		public function PongBumper2()		{			this.x = startX;			this.y = startY;			this.addEventListener(Event.ENTER_FRAME, onEnter);		}		public function onEnter(e:Event)		{			if (this.y != startY)			{				this.y = startY;			}			if (this.x < 0 + .5*(this.width))			{				this.x = .5*(this.width);			}			if (this.x > 600 - .5*(this.width))			{				this.x = 600 - .5*(this.width);			}		}	}}