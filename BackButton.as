package code{	import flash.display.MovieClip;	import flash.events.*;	public class BackButton extends MovieClip	{		public function BackButton(aX:Number, aY:Number)		{			this.x = aX;			this.y = aY;			this.addEventListener(MouseEvent.MOUSE_OVER, backOver);			this.scaleX = .5;			this.scaleY = .5;		}		public function backOver(e:MouseEvent)		{			this.buttonMode = true;		}	}}